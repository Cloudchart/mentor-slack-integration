{
  "development": {
    "use_env_variable": "MYSQL_URL",
    "pool": {
      "min":  0,
      "max":  5,
      "idle": 10000
    }
  },
  "production": {
    "use_env_variable": "DATABASE_URL",
    "pool": {
      "max":  20,
      "min":  0,
      "idle": 500
    }
  },
  "webApp": {
    "use_env_variable": "WEBAPP_DATABASE_URL",
    "pool": {
      "max":  20,
      "min":  0,
      "idle": 500
    }
  }
}
